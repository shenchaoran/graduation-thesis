@article{宋东泽2015一个生态传感网的,
  title={一个生态传感网的Web处理服务平台},
  author={宋东泽 and 罗万明 and 阎保平 and SongDongze and LuoWanming and YanBaoping},
  journal={科研信息化技术与应用},
  volume={6},
