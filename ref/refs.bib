 keywords={WPS;OGC;蒸散发;生态环境;传感网},
 abstract={随着近年来Web服务技术的日益成熟,越来越多的地理空间资源和处理功能以在线Web服务的形式发布在网络中,数据的共享和互操作性变得更加重要。WPS(Web Processing Service)便是由OGC(Open Geospatial Consortium)推出的空间信息的共享和互操作的规范,采用Web服务技术来实现空间数据处理的共享和互操作。本文介绍了我们基于OGC WPS标准开发的一个用于生态环境领域的空间数据处理平台,并选取对生态水文研究有重大意义的日均蒸散量计算模型验证了平台的可行性和实用性。},
}

@article{宋燕燕2006陆地碳循环模型的比较分析,
  title={陆地碳循环模型的比较分析},
  author={宋燕燕 and 王敬国 and 齐鑫 and 金红岩},
  journal={甘肃农业},
  number={5},
  pages={291-292},
  year={2006},
 keywords={陆地生态系统;碳循环;生物地理模型;生物地球化学模型},
 abstract={摘　要： 陆地碳循环是全球变化研究的重要内容。碳循环模型的研究已经成为陆地碳循环中最重要的组成成分．作为碳循环模型的两大类型，生物地理模型和生物化学模型在建模方法，输入参数。应用范围以及输出变量方面各不相同。本文通过对这两大类碳循环模型中的代表模型进行比较分析，指出未来模型发展的方向即综合两类模型的优点。利用遥感数据，建立以动态植被为基础的具有实时性的大尺度陆地碳循环模型。},
}

@article{孙艳玲2007应用动态植被模型,
  title={应用动态植被模型LPJ模拟中国植被变化研究},
  author={孙艳玲 and 延晓冬 and 谢德体 and 魏朝富},
  journal={西南大学学报(自然科学版)},
  volume={29},
  number={11},
  pages={86-92},
  year={2007},
 keywords={LPJ模型;气候变化;潜在植被;中国},
 abstract={依据中国植被与气候的关系对动态植被模型LPJ中的生物气候参数进行了调整.将改进后LPJ模拟的潜在植被类型与中国植被区划图比较发现有很大改进.应用CRU05资料和改进后的LPJ模型模拟了中国20世纪的4个气候阶段的潜在植被类型的分布变化.结果发现,由于气候变暖,20世纪我国森林植被大部分呈现出北移趋势,而热带季雨林、雨林植被分布在20世纪呈西移趋势,其面积减少最大;温带常绿阔叶林植被分布呈南移特征,其面积呈增加趋势.温带草本植被面积呈扩大趋势,而无植被区面积呈缩减趋势.},
}

@article{王超2006应用,
  title={应用BIOME-BGC模型研究典型生态系统的潜热通量——半干旱地区吉林通榆的模拟},
  author={王超 and 延晓冬 and 黄耀 and 刘辉志},
